    "lib",
    "src",
    "build",
    "data"
  ],
  "dependencies": {
    "@theia/core": "1.0.0",
    "@theia/monaco": "1.0.0",
    "sprotty-theia": "next",
    "mdr-sprotty": "0.0.0"
  },
  "devDependencies": {
    "cpr": "latest",
    "cross-zip-cli": "latest",
